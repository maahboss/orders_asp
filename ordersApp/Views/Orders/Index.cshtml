@model IEnumerable<ordersApp.Models.Order>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Orders</h2>

<table class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>Orders</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var order in Model)
        {
            <tr>
                <td>@order.Name</td>
            </tr>
        }
    </tbody>
</table>


